mixin StrokeState(_stateTitle, _num, _titleName, _boxName)
    .garage-stroke-box
        h3= _stateTitle
        textarea(id="stroke"+_num, class="stroke stroke"+_num,
        name="stroke"+_num, data-i=_num, data-stroke=_titleName)

        div.clear-area
            input(id="clear"+_num).clear(data-clear="clear"+_titleName, type="button" value="Delete" + " / " + _stateTitle)
